{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "delete": "rimraf ../src/main/resources/dist",
    "copy": "cpy ./dist/** ../src/main/resources --parents",
    "deleteAndCopy": "npm run delete && npm run copy",
    "rename": "rename ../src/main/resources/dist static",
    "buildAll": "npm run build && npm run deleteAndCopy"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "cpy-cli": "^2.0.0",
    "node-sass": "^4.9.0",
    "rename-cli": "^5.1.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  }
}
